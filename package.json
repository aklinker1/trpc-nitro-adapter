{
  "name": "trpc-nitro-adapter",
  "version": "0.7.0",
  "description": "A tRPC adapter for the nitro web server framework.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealroberts/trpc-nitro-adpater.git"
  },
  "author": "Micheal J. Roberts",
  "prepublish": "tsc",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "trpc",
    "nitro",
    "adapter"
  ],
  "engines": {
    "node": ">=18.12.1 < 20"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/trpc-nitro-adapter.umd.js",
  "module": "./dist/trpc-nitro-adapter.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/trpc-nitro-adapter.es.js",
      "require": "./dist/trpc-nitro-adapter.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "peerDependencies": {
    "@trpc/server": "^10.32.0",
    "h3": "^1.9.0",
    "ufo": "^1.3.0"
  },
  "peerDependenciesMeta": {
    "@trpc/server": {
      "optional": false
    },
    "h3": {
      "optional": false
    },
    "ufo": {
      "optional": false
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@trpc/server": "^10.32.0",
    "@types/node": "^20.2.1",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "h3": "^1.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "rollup": "^3.22.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "ufo": "^1.3.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
